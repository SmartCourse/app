{
  "name": "smartcourse-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npx nodemon ./src/bin/www",
    "lint": "npx eslint src/**/*.js tests/**/*.js",
    "test:src": "npx nyc mocha tests/**/*.js",
    "test": "npm run lint && npm run test:src",
    "coverage": "npx nyc report --reporter=html --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0",
    "sqlite3": "^4.0.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "eslint": "^5.3.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "nyc": "^13.0.1"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  }
}
