{
  "name": "smartcourse-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npx nodemon ./src/bin/www",
    "lint": "npx eslint src/**/*.js tests/**/*.js",
    "fix-lint": "npx eslint src/**/*.js tests/**/*.js --fix",
    "test:src": "npx mocha tests/**/*.js",
    "test": "npm run lint && npm run test:src"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0",
    "sqlite3": "^4.0.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  }
}
